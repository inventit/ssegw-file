{
  "name": "file",
  "version": "1.1.0",
  "description": "Deliver and fetch a file",
  "maintainers": [
    {
      "name": "Inventit Inc.",
      "email": "egypt@yourinventit.com"
    }
  ],
  "bugs": {
    "mail": "egypt@yourinventit.com"
  },
  "dependencies": {
    "moat": ">=1.0.0 <2.0.0"
  },
  "main" : "file.so",
  "models" : {
    "ContentInfo" : {
      "array" : false,
      "scope" : "device",
      "attributes" : {
	"deliveryUrl" : {"type" : "string"},
	"uploadUrl" : {"type" : "string"},
	"name" : {"type" : "string"},
	"destinationPath" : {"type" : "string"},
	"sourcePath" : {"type" : "string"}
      },
      "commands" : {
	"download" : {"paramType" : null},
	"upload" : {"paramType" : null}
      }
    },
    "FileResult" : {
      "array" : false,
      "scope" : "device",
      "attributes" : {
	"success" : {"type" : "boolean"},
	"message" : {"type" : "string"},
	"code" : {"type" : "string"},
	"uid" : {"type" : "string"}
	
      }
    }
  }
}
